{
  "name": "projet-05-o-world-back",
  "version": "1.0.0",
  "description": "environment back for the O'World app",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js | bunyan -o short",
    "start": "node index.js",
    "db:create": "sqitch revert -y && sqitch deploy",
    "db:seed": "psql oworld -f data/seedingCountry.sql && psql oworld -f data/seedingRBAC.sql && node data/faker.js && psql oworld -f data/seedingWorld.sql",
    "db:reset": "npm run db:create && npm run db:seed | bunyan -o short",
    "test": "jest",
    "docs": "jsdoc -c jsdoc.config.json"
  },
  "author": "Lo√Øc Dubrion",
  "license": "ISC",
  "devDependencies": {
    "clean-jsdoc-theme": "^4.2.7",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "faker": "^5.5.3",
    "jest": "^29.5.0",
    "joi": "^17.9.2",
    "jsdoc": "^4.0.2",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "pg": "^8.11.0",
    "radio-browser": "^2.2.3"
  }
}
